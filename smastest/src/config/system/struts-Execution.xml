<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="execution" extends="global" namespace="/execution">
		
		<action name="toExecute" class="executionAction" method="toExecute">
			<result>/system/execution/toExecute.jsp</result>
		</action>
		
		<action name="execute" class="executionAction" method="execute">
			<result name="success">/system/execution/executionSuccess.jsp</result>
			<result name="error">/system/execution/executionFailed.jsp</result>
		</action>
		
	</package>

</struts>
