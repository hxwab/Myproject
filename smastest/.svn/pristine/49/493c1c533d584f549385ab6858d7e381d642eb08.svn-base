package csdc.bean;

public class Tes {

	String name;
	String uni;
	int flag;
	public Tes(String name, String uni) {
		this.name = name;
		this.uni = uni;
	}
	public Tes(String name, String uni, int flag) {
		this.name = name;
		this.uni = uni;
		this.flag = flag;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getUni() {
		return uni;
	}
	public void setUni(String uni) {
		this.uni = uni;
	}
	public int getFlag() {
		return flag;
	}
	public void setFlag(int flag) {
		this.flag = flag;
	}
	
}
