package csdc.tool;

/**
 * 专家树条目
 * @author fengcl
 *
 */
public class ExpertTreeItem {
	public String id;				//专家id
	public String name;				//专家姓名
	public String specialityTitle;	//专家职称
	public String discipline;		//专家学科代码
	public String pojcnt;			//专家参评项目数量
	public String university;		//所属高校名称
	public String universityAcronym;//高校名称首字母缩写
	public String universityReviewerCnt;//所属高校的参评专家总人数
	
	/**
	 * 专家树条目构造器
	 * @param id	专家id
	 * @param name	专家姓名
	 * @param title	专家职称
	 * @param discipline	专家的学科代码
	 * @param pojcnt	专家参评项目数
	 * @param university	专家所属高校
	 * @param universityAcronym	高校名称首字母缩写
	 * @param universityReviewerCnt 所属高校的参评专家总人数
	 */
	public ExpertTreeItem(String id, String name, String specialityTitle,
			String discipline, String pojcnt, String university, String universityAcronym, String universityReviewerCnt) {
		super();
		this.id = id;
		this.name = name;
		this.specialityTitle = specialityTitle;
		this.discipline = discipline;
		this.pojcnt = pojcnt;
		this.university = university;
		this.universityAcronym = universityAcronym;
		this.universityReviewerCnt = universityReviewerCnt;
	}
}
