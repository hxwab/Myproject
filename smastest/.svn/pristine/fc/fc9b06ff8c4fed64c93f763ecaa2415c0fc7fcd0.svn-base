/**
 * 弹出层选择项目
 * @param {Object} disType
 * @param {Object} area
 * @param {Object} proType
 */
function popProject(args){
	new top.PopLayer({"title" : args.title, "src" : args.src, "inData" : args.inData,
		"document" : top.document, "callBack" : args.callBack});
};

