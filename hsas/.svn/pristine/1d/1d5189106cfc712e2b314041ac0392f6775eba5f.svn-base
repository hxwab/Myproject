<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="firstReviewAudit" extends="main" namespace="/product/firstReviewAudit">
	
		<action name="toList" class="firstReviewAuditAction" method="toList">
			<result>/jsp/product/firstReviewAudit/list.jsp</result>
		</action>

		<action name="list" class="firstReviewAuditAction" method="list">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="toView" class="firstReviewAuditAction" method="toView">
			<result>/jsp/product/firstReviewAudit/view.jsp</result>
		</action>

		<action name="view" class="firstReviewAuditAction" method="view">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="back" class="firstReviewAuditAction" method="back">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="simpleSearch" class="firstReviewAuditAction" method="simpleSearch">
			<result type="chain">list</result>
		</action>
		
		<action name="changeYear" class="firstReviewAuditAction" method="changeYear">
			<result type="chain">list</result>
		</action>
		
		<action name="toPage" class="firstReviewAuditAction" method="toPage">
			<result type="chain">list</result>
		</action>
		
		<action name="sort" class="firstReviewAuditAction" method="sort">
			<result type="chain">list</result>
		</action>
		
		<action name="changePageSize" class="firstAuditAction" method="changePageSize">
			<result type="chain">list</result>
		</action>
		
		<action name="advSearch" class="firstReviewAuditAction" method="advSearch">
			<result type="chain">list</result>
		</action>
		
		<action name="addAuditResult" class="firstReviewAuditAction" method="addAuditResult">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="audit" class="firstReviewAuditAction" method="audit">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<!-- 导出 -->
	    <action name="exportListData" class="firstReviewAuditAction" method="exportListData">
        	<result name="success" type="stream">
				<param name="contentType">application/vnd.ms-excel</param>
				<param name="contentDisposition">attachment;fileName="${fileName}"</param>
				<param name="inputName">listDataStream</param>
			</result>
	   	</action>	
		</package>
</struts>