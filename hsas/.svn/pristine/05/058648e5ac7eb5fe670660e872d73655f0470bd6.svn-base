<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="data" extends="main" namespace="/data">
	
		<action name="toCaclResult" class="productViewAction" method="toCaclResult">
			<result>/jsp/data/view.jsp</result>
		</action>

		<action name="caclResult" class="productViewAction" method="caclResult">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
	</package>

</struts>