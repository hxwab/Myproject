<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="agency" extends="main" namespace="/system/dataDic/agency">
		<action name="toList" class="agencyAction" method="toList">
			<result>/jsp/system/dataDic/agency/list.jsp</result>
		</action>
		
		<action name="list" class="agencyAction" method="list">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="toAdd" class="agencyAction" method="toAdd">
			<result>/jsp/system/dataDic/agency/add.jsp</result>
		</action>
		
		<action name="add" class="agencyAction" method="add">
			<result type="json">
				<param name="root">jsonMap</param>
			</result>
		</action>
		
		<action name="toView" class="agencyAction" method="toView">
			<result>/jsp/system/dataDic/agency/view.jsp</result>
		</action>

		<action name="view" class="agencyAction" method="view">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		<action name="toModify" class="agencyAction" method="toModify">
			<result>/jsp/system/dataDic/agency/modify.jsp</result>
		</action>

		<action name="modify" class="agencyAction" method="modify">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		<action name="delete" class="agencyAction" method="delete">
			<result type="json"><param name="root">jsonMap</param></result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="toPage" class="agencyAction" method="toPage">
			<result type="chain">list</result>
		</action>
		<action name="sort" class="agencyAction" method="sort">
			<result type="chain">list</result>
		</action>
		
		<action name="simpleSearch" class="agencyAction" method="simpleSearch">
			<result type="chain">list</result>
		</action>
		
		<action name="advSearch" class="agencyAction" method="advSearch">
			<result type="chain">list</result>
		</action>
		
		
		<action name="prev" class="agencyAction" method="prev">
			<result type="redirectAction">
				<param name="actionName">view</param>
				<param name="entityId">${entityId}</param>
			</result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>

		<action name="next" class="agencyAction" method="next">
			<result type="redirectAction">
				<param name="actionName">view</param>
				<param name="entityId">${entityId}</param>
			</result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
	</package>
</struts>