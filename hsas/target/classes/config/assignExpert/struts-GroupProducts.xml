<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="groupProducts" extends="main" namespace="/groupProducts">
	
		<action name="list" class="groupProductsAction" method="list">
			<result type="json"><param name="root">jsonMap</param> </result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="toPage" class="groupProductsAction" method="toPage">
			<result type="chain">list</result>
		</action>
		
		<action name="sort" class="groupProductsAction" method="sort">
			<result type="chain">list</result>
		</action>
	</package>
</struts>
