<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="chiefReviewProduct" extends="main" namespace="/chiefReviewProduct">
	
		<action name="list" class="chiefReviewProductAction" method="list">
			<result type="json"><param name="root">jsonMap</param> </result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		
		<action name="toPage" class="chiefReviewProductAction" method="toPage">
			<result type="chain">list</result>
		</action>
		
		<action name="sort" class="chiefReviewProductAction" method="sort">
			<result type="chain">list</result>
		</action>
		
		<action name="simpleSearch" class="chiefReviewProductAction" method="simpleSearch">
			<result type="chain">list</result>
		</action>
		
		<action name="getChiefReviewProducts" class="chiefReviewProductAction" method="getChiefReviewProducts">
			<result type="json"><param name="root">jsonMap</param> </result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
		<action name="assignChiefProducts" class="chiefReviewProductAction" method="assignChiefProducts">
			<result type="json"><param name="root">jsonMap</param> </result>
			<result name="input" type="json"><param name="root">jsonMap</param></result>
		</action>
	</package>
</struts>
